{
  "manifest_version": 3,
  "name": "ClickUp Track",
  "description": "ClickUp Track is a Chrome extension that boosts productivity by integrating time tracking directly into your ClickUp workflow.",
  "version": "1.0.0",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "action": {
    "default_popup": "dist/index.html",
    "default_icon": {
      "16": "icons/16.png",
      "48": "icons/48.png",
      "128": "icons/128.png"
    }
  },
  "options_page": "dist/index.html",
  "commands": {
    "_execute_action": {
      "suggested_key": {
        "default": "Alt+C"
      }
    }
  },
  "background": {
    "service_worker": "sw.js",
    "type": "module"
  },
  "permissions": ["alarms", "windows", "storage"],
  "key": ""
}
